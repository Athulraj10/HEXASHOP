  <script>
    // Get the chart element
var chartElement = document.getElementById('myChart');

// Create the initial chart with some data
var myChart = new Chart(chartElement, {
  type: 'bar',
  data: {
    labels: ['Total sale', 'current Revenue', 'Daily Income', 'monthly Income'],
    datasets: [{
      label: "<%=data.totalOrderAmount%> of Votes",
      data: [parseFloat('<%= data.totalOrderAmount %>'),parseFloat('<%= data.totalRevenue %>'),parseFloat('<%= data.totalSaleTotalAmount %>'),parseFloat('<%= data.totalSaleAmountsMonthly %>')],
      backgroundColor: [
        'rgba(255, 99, 132, 0.2)',
        'rgba(54, 162, 235, 0.2)',
        'rgba(255, 206, 86, 0.2)',
        'rgba(75, 192, 192, 0.2)',
        'rgba(153, 102, 255, 0.2)',
        'rgba(255, 159, 64, 0.2)'
      ],
      borderColor: [
        'rgba(255, 99, 132, 1)',
        'rgba(54, 162, 235, 1)',
        'rgba(255, 206, 86, 1)',
        'rgba(75, 192, 192, 1)',
        'rgba(153, 102, 255, 1)',
        'rgba(255, 159, 64, 1)'
      ],
      borderWidth: 1
    }]
  },
  options: {
    responsive: true,
    scales: {
      y: {
        beginAtZero: true
      }
    }
  }
});

// Update the chart data dynamically
function updateChartData() {
  // Example of updating chart data
  var newData = [5, 10, 15, 20, 25, 30];
  myChart.data.datasets[0].data = newData;
  myChart.update();
}

// Call the updateChartData function whenever you want to update the chart
updateChartData();

  </script>



  <script>
    $(document).ready(function () {
      // Block or Unblock user on button click
      $('.block-button').on('click', function () {
        var userId = $(this).data('userid');
        var blockStatus = $(this).data('blockstatus');

        // AJAX request to block/unblock user
        $.ajax({
          method: 'POST',
          url: '/admin/admin/block', // Replace with the appropriate URL for your backend route
          data: { userId: userId, blockStatus: blockStatus },
          success: function (response) {
            // Handle the response from the server
            // Assuming the server responds with updated user data

            // Update the button text and class based on the updated block status
            if (response.block === false) {
              $('#block-button-' + userId)
                .removeClass('btn-danger')
                .addClass('btn-success')
                .text('Active')
                .data('blockstatus', 0);
            } else {
              $('#block-button-' + userId)
                .removeClass('btn-success')
                .addClass('btn-danger')
                .text('Blocked')
                .data('blockstatus', 1);
            }
          },
          error: function (error) {
            console.log(error);
          }
        });
      });
    });
  </script>
 
